#include <iostream>
#include <math.h>
using namespace std;    

int main ()
{
    int num1,num2,num3,max;
    cout<<"Enter 3 number " << endl;
    cout<<"-->";
    cin>>num1;
    cout<<"-->";
    cin>>num2;
    cout<<"-->";
    cin>>num3;
    if(num1>num2)
        max=num1;
    else
        max=num2;
    if(max<num3)
        max=num3;
    cout<<"Max number : "<<max<<endl;
}
